        function setarvdesi(str) {
            var k = 0;
            var chcomp = str[0];
            k++;
            while (k < str.length - 1) {
                var arv = str[k].split('#');
                for (var i = 13; i <= 77; i += 7) {
                    if (arv[i]) {
                        if (chcomp.search(arv[i]) === -1) {
                            chcomp = str[k];
                        }
                    }
                }
                k++;
            }
            if (chcomp) {
                var arv_desi = '';
                var chcomp = chcomp.split('#');
                for (i = 13; i <= 77; i += 7) {
                    if (chcomp[i]) {
                        if (arv_desi)
                        {
                            arv_desi += '+' + chcomp[i];
                        }
                        else
                        {
                            arv_desi = chcomp[i];
                        }
                    }
                }
            }
            if ($('#Arv_Lign').val() != 0) {
                var ligne = parseInt($('#Arv_Lign').val());
                if (ligne === 1) {
                    arv_desi = '1ère ligne ' + arv_desi;
                }
                if (ligne > 1) {
                    arv_desi = ligne + 'ème ligne ' + arv_desi;
                }
            }
            else {
                $('#Arv_Lign').val('1');
                arv_desi = '1ère ligne ' + arv_desi;
            }
            return(arv_desi);
        }
        function setarv(str) {

            for (var i = 0; i < str.length - 1; i++) {
                var val = str[i].split('#');
                var cpt = 0;
                for (var j = 13; j <= 77; j += 7) {
                    if (val[j]) {
                        $('#Arv' + cpt).val(val[j]);
                    }
                    cpt++;
                }
            }

        }

